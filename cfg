//disable some search engines
removeSearchAliasX('g'); // google
removeSearchAliasX('b'); // baidu
removeSearchAliasX('w'); // bing

addSearchAliasX('e', 'eng dict.leo.org', 'https://dict.leo.org/englisch-deutsch/{0}');
addSearchAliasX('f', 'fr dict.leo.org', 'https://dict.leo.org/franz√∂sisch-deutsch/{0}');
addSearchAlias('se', 'eng dict.leo.org', 'https://dict.leo.org/englisch-deutsch/{0}');
addSearchAlias('sf', 'fr dict.leo.org', 'https://dict.leo.org/englisch-deutsch/{0}');
